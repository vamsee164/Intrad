{
  "firestore": {
    "database": "(default)",
    "location": "asia-east2",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
 
  "hosting": {
    "public": "dist/intra-d/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }


}
